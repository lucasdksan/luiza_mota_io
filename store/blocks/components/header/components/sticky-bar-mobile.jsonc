{
    "sticky-layout#header-mobile": {
        "title": "Sticky layout do header mobile",
        "props": {
            "blockClass": "header-mobile"
        },
        "children": [
            "flex-layout.row#header-sticky-bar-mobile"
        ]
    },
    "flex-layout.row#header-sticky-bar-mobile": {
        "title": "Barra de sticky mobile",
        "props": {
            "fullWidth": true,
            "preserveLayoutOnMobile": true,
            "preventHorizontalStretch": true,
            "blockClass": [
                "header-sticky-bar-mobile"
            ]
        },
        "children": [
            "flex-layout.row#header-sticky-bar-mobile-first-line",
            "flex-layout.row#header-sticky-bar-mobile-second-line"
        ]
    },
    "flex-layout.row#header-sticky-bar-mobile-first-line": {
        "title": "Primeira linha do header",
        "props": {
            "fullWidth": true,
            "preserveLayoutOnMobile": true,
            "preventHorizontalStretch": true,
            "blockClass": [
                "header-sticky-bar-mobile-first-line"
            ]
        },
        "children": [
            "two7-menu-mobile",
            "two7-logo-custom",
            "flex-layout.row#right-side-header-mobile"
        ]
    },
    "flex-layout.row#right-side-header-mobile": {
        "title": "Lado direito do header mobile",
        "props": {
            "blockClass": "right-side-header-mobile"
        },
        "children": [
            "two7-search-mobile-custom",
            "minicart.v2"
        ]
    },
    "flex-layout.row#header-sticky-bar-mobile-second-line": {
        "title": "Segunda linha do header",
        "props": {
            "fullWidth": true,
            "preserveLayoutOnMobile": true,
            "preventHorizontalStretch": true,
            "blockClass": [
                "header-sticky-bar-mobile-second-line"
            ]
        },
        "children": [
            "search-bar#search-bar-mobile"
        ]
    }
}