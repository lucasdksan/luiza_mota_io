    "eslint-plugin-prettier": "4.x",
    "glob": "^10.3.3",
    "postcss": "8.x",
    "postcss-cli": "10.x",
    "postcss-pxtorem": "^6.0.0",
    "prettier": "2.x",
    "sass": "1.x"
  },
  "browserslist": [
    "> 2%",
    "last 5 versions",
    "not dead"
